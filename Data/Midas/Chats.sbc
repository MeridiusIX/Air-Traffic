    <EntityComponents>

		<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
            <Id>
                <TypeId>Inventory</TypeId>
                <SubtypeId>RAI-Chat-AirTraffic-MidasHostilePlayerNear</SubtypeId>
            </Id>
            <Description>
			
			[RivalAI Chat]
			
			[UseChat:true]
            [StartsReady:true]
            [MaxChats:3]
            [BroadcastRandomly:true]
			[Author:MIDAS Dispatch]
			[Color:Red]

            [ChatMessages:MIDAS Dispatch: Dispatching additional support.]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Notify]
			
			[ChatMessages:MIDAS Dispatch: Reinforcements enroute.]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Notify]
			
			[ChatMessages:MIDAS Dispatch: Hostile presence detected.]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Notify]
			
			</Description>
        </EntityComponent>
		
