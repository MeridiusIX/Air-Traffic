            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Chat]
			
			[ChatMessages:Leave me alone you violent brute!]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Chat]
			
			</Description>
        </EntityComponent>
		
