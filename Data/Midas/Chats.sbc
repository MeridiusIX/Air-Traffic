            [BroadcastChatType:Chat]
			
			</Description>
        </EntityComponent>
		
		<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
            <Id>
                <TypeId>Inventory</TypeId>
                <SubtypeId>RAI-Chat-AirTraffic-MidasTransportCourierPlayerNear1500</SubtypeId>
            </Id>
            <Description>
			
			[RivalAI Chat]
			
			[UseChat:true]
            [StartsReady:true]
            [MaxChats:3]
			[MinTime:10]
			[MaxTime:11]
            [BroadcastRandomly:false]
			[Author:{AntennaName}]
			[Color:Red]

            [ChatMessages:You've come to take my gold, haven't you? Well you can't have it! Maximum forward thrust!]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Chat]
			
			[ChatMessages:You'll never catch me!]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Chat]
			
			[ChatMessages:Stop chasing me! High speeds make me queasy!]
            [ChatAudio:ArcHudGPSNotification2]
            [BroadcastChatType:Chat]
			
			</Description>
        </EntityComponent>
		
