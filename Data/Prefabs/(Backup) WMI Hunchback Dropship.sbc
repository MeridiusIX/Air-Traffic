const string mountMergeNameTag = "Mount";  
const string dropPodMergeNameTag = "Drop Pod";  
const string textScreenName = "Drop Status";  
const string hangarDoorGroupName = "Systems - Hangar Door"; 
  
const int numberOfDropPods = 12;  
  
const char ready = '\uE118';  
const char dropped = '\uE1C0';  
  
string dropPodStatusTemplate = " Drop Pod Status"  
+   "\n----------------------"  
+   $"\n Pod 1 :{ready} Pod 7 :{ready}"  
+   $"\n Pod 2 :{ready} Pod 8 :{ready}"  
+   $"\n Pod 3 :{ready} Pod 9 :{ready}"  
+ $"\n\n Pod 4 :{ready} Pod 10:{ready}"  
+   $"\n Pod 5 :{ready} Pod 11:{ready}"  
+   $"\n Pod 6 :{ready} Pod 12:{ready}"; 
  
const double runsPerSecond = 5;  
const double timeMaxCycle = 1 / runsPerSecond;  
double timeCurrentCycle = 0;  
  
void Main()  
{  
    timeCurrentCycle += Runtime.TimeSinceLastRun.TotalSeconds;  
      
    if (timeCurrentCycle &lt; timeMaxCycle)  
    {  
        return;  
    }  
      
    timeCurrentCycle = 0;  
      
    Echo("WMI Drop Status Script Online... " + RunningSymbol());  
      
    string dropPodStatus = dropPodStatusTemplate;  
  
    for (int i = 1; i &lt;= numberOfDropPods; i++)  
    {  
        var mountMerges = new List&lt;IMyShipMergeBlock&gt;();  
        GridTerminalSystem.GetBlocksOfType(mountMerges, block =&gt; block.CustomName.Contains($"&lt;{mountMergeNameTag} {i}&gt;"));  
          
        if (mountMerges.Count == 0)  
        {  
            Echo($"Error: Mount merge {i} was not found... skipping");  
            continue;  
        }  
          
        var thisMerge = mountMerges[0];  
  
        IMyShipMergeBlock dropPodMerge = GetClosestBlockOfType&lt;IMyShipMergeBlock&gt;(thisMerge, dropPodMergeNameTag, 2.5);  
  
        if (dropPodMerge == null)  
        {  
            if (i &lt; 10)      
                dropPodStatus = dropPodStatus.Replace($"{i} :{ready}", $"{i} :{dropped}");  
            else  
                dropPodStatus = dropPodStatus.Replace($"{i}:{ready}", $"{i}:{dropped}");  
              
            //Echo($"Drop pod {i} disconnected");  
        }  
        else  
        {  
            //Echo($"Drop pod {i} attached");  
        }  
    } 
     
    var hangarDoors = new List&lt;IMyAirtightHangarDoor&gt;(); 
    GridTerminalSystem.GetBlockGroupWithName(hangarDoorGroupName).GetBlocksOfType(hangarDoors); 
    if (hangarDoors.Count != 0) 
    { 
        var hangarDoor = hangarDoors[0]; 
        dropPodStatus += $"\n\n Drop Bay: {hangarDoor.Status}"; 
    } 
  
    WriteToTextPanel(textScreenName, dropPodStatus);  
}  
  
//Whip's Get Closest Block of Type Method - 10/14/16  
T GetClosestBlockOfType&lt;T&gt;(IMyShipMergeBlock reference, string name = "", double maxDistance = 100000) where T : class, IMyTerminalBlock  
{  
  
    var allBlocks = new List&lt;T&gt;();  
  
    if (name == "")  
        GridTerminalSystem.GetBlocksOfType(allBlocks);  
    else  
        GridTerminalSystem.GetBlocksOfType(allBlocks, block =&gt; block.CustomName.Contains(name));  
  
    if (allBlocks.Count == 0)  
    {  
        return null;  
    }  
  
    var closestBlock = allBlocks[0];  
    var shortestDistance = Vector3D.DistanceSquared(reference.GetPosition(), closestBlock.GetPosition());  
    allBlocks.Remove(closestBlock); //remove this block from the list  
  
    foreach (T thisBlock in allBlocks)  
    {  
        var thisDistance = Vector3D.DistanceSquared(reference.GetPosition(), thisBlock.GetPosition());  
  
        if (thisDistance &lt; shortestDistance)  
        {  
            closestBlock = thisBlock;  
            shortestDistance = thisDistance;  
        }  
        //otherwise move to next one  
    }  
      
    //Echo($"{name}: {shortestDistance}");  
  
    if (maxDistance * maxDistance &gt;= shortestDistance)  
        return closestBlock;  
    else  
        return null;  
}  
  
  
void WriteToTextPanel(string textPanelName, string textToWrite, bool append = false)  
{  
    var listScreens = new List&lt;IMyTerminalBlock&gt;();  
    GridTerminalSystem.SearchBlocksOfName(textPanelName, listScreens, block =&gt; block is IMyTextPanel);  
    if (listScreens.Count == 0)  
    {  
        Echo("Error: No text panel with name tag '" + textPanelName + "' was found");  
        return;  
    }  
    else  
    {  
        for (int i = 0; i &lt; listScreens.Count; i++)  
        {  
            var thisScreen = listScreens[i] as IMyTextPanel;  
            if (thisScreen != null)  
            {  
                thisScreen.WritePublicText(textToWrite, append);  
                thisScreen.ShowPublicTextOnScreen();  
            }  
        }  
    }  
}  
  
  
