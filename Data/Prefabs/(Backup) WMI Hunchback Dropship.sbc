{ 
    configSB.Clear(); 
    foreach (var keyValue in configDict) 
    { 
        configSB.AppendLine($"{keyValue.Key} = {keyValue.Value}"); 
    } 
 
    block.CustomData = configSB.ToString(); 
} 
 
