Vector3D VectorProjection(Vector3D a, Vector3D b) //proj a on b     
{ 
    Vector3D projection = a.Dot(b) / b.LengthSquared() * b; 
    return projection; 
} 
 
#region GYRO ORIENTATION 
//Whip's Gyro Orientation Method v4 - 6/2/17 
//Changed to use dictionary instead of array for redundancy incase blocks are destroyed 
//Also uses new world matrix workaround 
 
Dictionary&lt;IMyGyro, string&gt; gyroRelativeYaw = new Dictionary&lt;IMyGyro, string&gt;(); 
Dictionary&lt;IMyGyro, string&gt; gyroRelativePitch = new Dictionary&lt;IMyGyro, string&gt;(); 
Dictionary&lt;IMyGyro, string&gt; gyroRelativeRoll = new Dictionary&lt;IMyGyro, string&gt;(); 
Dictionary&lt;IMyGyro, int&gt; gyroYawSign = new Dictionary&lt;IMyGyro, int&gt;(); 
Dictionary&lt;IMyGyro, int&gt; gyroPitchSign = new Dictionary&lt;IMyGyro, int&gt;(); 
Dictionary&lt;IMyGyro, int&gt; gyroRollSign = new Dictionary&lt;IMyGyro, int&gt;(); 
 
void GetGyroOrientation(IMyTerminalBlock reference_block, List&lt;IMyGyro&gt; gyro_list) 
{ 
    gyroRelativeYaw = new Dictionary&lt;IMyGyro, string&gt;(gyro_list.Count); 
    gyroRelativePitch = new Dictionary&lt;IMyGyro, string&gt;(gyro_list.Count); 
    gyroRelativeRoll = new Dictionary&lt;IMyGyro, string&gt;(gyro_list.Count); 
 
    gyroYawSign = new Dictionary&lt;IMyGyro, int&gt;(gyro_list.Count); 
    gyroPitchSign = new Dictionary&lt;IMyGyro, int&gt;(gyro_list.Count); 
    gyroRollSign = new Dictionary&lt;IMyGyro, int&gt;(gyro_list.Count); 
 
    var referenceMatrix = GetWorldMatrix(reference_block); 
    var reference_up = referenceMatrix.Up; //assuming rot right 
    var reference_right = referenceMatrix.Right; //assuming rot up 
    var reference_forward = referenceMatrix.Forward; //assuming rot up 
 
    foreach (var thisGyro in gyro_list) 
    { 
        var gyroMatrix = GetWorldMatrix(thisGyro); 
        var gyro_forward = gyroMatrix.Forward; 
        var gyro_backward = gyroMatrix.Backward; 
        var gyro_up = gyroMatrix.Up; 
        var gyro_down = gyroMatrix.Down; 
        var gyro_left = gyroMatrix.Left; 
        var gyro_right = gyroMatrix.Right; 
 
        /// Pitch Fields ///    
        if (reference_right == gyro_forward) 
        { 
            gyroRelativePitch.Add(thisGyro, "Roll"); 
            gyroPitchSign.Add(thisGyro, 1); 
        } 
        else if (reference_right == gyro_backward) 
        { 
            gyroRelativePitch.Add(thisGyro, "Roll"); 
            gyroPitchSign.Add(thisGyro, -1); 
        } 
        else if (reference_right == gyro_right) 
        { 
            gyroRelativePitch.Add(thisGyro, "Pitch"); 
            gyroPitchSign.Add(thisGyro, 1); 
        } 
        else if (reference_right == gyro_left) 
        { 
            gyroRelativePitch.Add(thisGyro, "Pitch"); 
            gyroPitchSign.Add(thisGyro, -1); 
        } 
        else if (reference_right == gyro_up) 
        { 
            gyroRelativePitch.Add(thisGyro, "Yaw"); 
            gyroPitchSign.Add(thisGyro, -1); 
        } 
        else if (reference_right == gyro_down) 
        { 
            gyroRelativePitch.Add(thisGyro, "Yaw"); 
            gyroPitchSign.Add(thisGyro, 1); 
        } 
 
        /// Yaw Fields /// 
        if (reference_up == gyro_forward) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Roll"); 
            gyroYawSign.Add(thisGyro, -1); 
        } 
        else if (reference_up == gyro_backward) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Roll"); 
            gyroYawSign.Add(thisGyro, 1); 
        } 
        else if (reference_up == gyro_right) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Pitch"); 
            gyroYawSign.Add(thisGyro, -1); 
        } 
        else if (reference_up == gyro_left) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Pitch"); 
            gyroYawSign.Add(thisGyro, 1); 
        } 
        else if (reference_up == gyro_up) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Yaw"); 
            gyroYawSign.Add(thisGyro, 1); 
        } 
        else if (reference_up == gyro_down) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Yaw"); 
            gyroYawSign.Add(thisGyro, -1); 
        } 
 
        /// Roll Fields /// 
        if (reference_forward == gyro_forward) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Roll"); 
            gyroRollSign.Add(thisGyro, 1); 
        } 
        else if (reference_forward == gyro_backward) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Roll"); 
            gyroRollSign.Add(thisGyro, -1); 
        } 
        else if (reference_forward == gyro_right) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Pitch"); 
            gyroRollSign.Add(thisGyro, 1); 
        } 
        else if (reference_forward == gyro_left) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Pitch"); 
            gyroRollSign.Add(thisGyro, -1); 
        } 
        else if (reference_forward == gyro_up) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Yaw"); 
            gyroRollSign.Add(thisGyro, -1); 
        } 
        else if (reference_forward == gyro_down) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Yaw"); 
            gyroRollSign.Add(thisGyro, 1); 
        } 
    } 
} 
 
void ApplyGyroOverride(double pitch_speed, double yaw_speed, double roll_speed, List&lt;IMyGyro&gt; gyro_list) 
{ 
    foreach (var thisGyro in gyro_list) 
    { 
        string relativeYaw, relativePitch, relativeRoll; 
        int yawSign, pitchSign, rollSign; 
 
        bool test1 = gyroRelativeYaw.TryGetValue(thisGyro, out relativeYaw); 
        bool test2 = gyroRelativePitch.TryGetValue(thisGyro, out relativePitch); 
        bool test3 = gyroRelativeRoll.TryGetValue(thisGyro, out relativeRoll); 
        bool test4 = gyroYawSign.TryGetValue(thisGyro, out yawSign); 
        bool test5 = gyroPitchSign.TryGetValue(thisGyro, out pitchSign); 
        bool test6 = gyroRollSign.TryGetValue(thisGyro, out rollSign); 
 
        if (test1 &amp;&amp;test2 &amp;&amp; test3 &amp;&amp; test4 &amp;&amp; test5 &amp;&amp; test6) 
        { 
            thisGyro.SetValue&lt;float&gt;(relativeYaw, (float)yaw_speed * yawSign); 
            thisGyro.SetValue&lt;float&gt;(relativePitch, (float)pitch_speed * pitchSign); 
            thisGyro.SetValue&lt;float&gt;(relativeRoll, (float)roll_speed * rollSign); 
            thisGyro.SetValue("Override", true); 
        } 
    } 
} 
