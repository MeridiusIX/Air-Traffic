double descentSpeed = 5; //The speed (m/s) that the drop pod will descend at once it has begun braking  
double shutdownTime = 3; //Time (in seconds) that the drop pod must remain stationary before the code shuts down 
bool useDriftCompensation = true; //If the drop pod should compensate for lateral drift when braking 
bool useMergeForDropPod = false; //This will stop the code from executing without a merge if TRUE 
bool attemptToLand = true; //If the code should attempt to land the drop pod or stop slightly above the ground 
bool disableThrustOnLanding = false; //If the thrusters will turn off after a successful landing 
                                     //This is only considered if attemptToLand is TRUE 
 
//////////////////////////////////////////////////// 
//================================================= 
//No touchey anything below here 
//================================================= 
//////////////////////////////////////////////////// 
 
const double updatesPerSecond = 10; 
const double timeMaxCycle = 1 / updatesPerSecond; 
 
double timeSpentStationary = 0; 
double timeCurrentCycle = 100; 
double timeSinceDrop = 0; 
double angleRoll = 0; 
double anglePitch = 0; 
double currentSpeed = 0; 
double altitude = 0; 
double downSpeed = 0; 
double pitch_deg = 0; 
double roll_deg = 0; 
double brakeAltitudeThreshold = 0; 
double stabilizeAltitudeThreshold = 0; 
double gravityVecMagnitude = 0; 
 
bool shouldDrop = false; 
bool successfulDrop = false; 
bool shouldBrake = false; 
bool shouldStabilize = false; 
bool isSetup = false; 
bool safeToActivate = false; 
bool shouldTrigger = false; 
 
string brakeStatus = "&gt;&gt; Disabled &lt;&lt;"; 
string stableStatus = "&gt;&gt; Disabled &lt;&lt;"; 
 
Vector3D shipVelocityVec = new Vector3D(0, 0, 0); 
Vector3D gravityVec = new Vector3D(0, 0, 0); 
 
IMyShipController referenceBlock = null; 
IMyTimerBlock timer = null; 
 
List&lt;IMyGyro&gt; gyros = new List&lt;IMyGyro&gt;(); 
List&lt;IMyThrust&gt; allThrusters = new List&lt;IMyThrust&gt;(); 
List&lt;IMyThrust&gt; brakingThrusters = new List&lt;IMyThrust&gt;(); 
List&lt;IMyThrust&gt; otherThrusters = new List&lt;IMyThrust&gt;(); 
List&lt;IMyShipController&gt; shipControllers = new List&lt;IMyShipController&gt;(); 
List&lt;IMyTextPanel&gt; statusScreens = new List&lt;IMyTextPanel&gt;(); 
List&lt;IMyShipMergeBlock&gt; merges = new List&lt;IMyShipMergeBlock&gt;(); 
List&lt;IMyTimerBlock&gt; landingTimers = new List&lt;IMyTimerBlock&gt;(); 
 
Program() 
{ 
    BuildConfig(Me); 
} 
 
void Main(string arg) 
{ 
    if (arg.ToLower() == "drop") 
    { 
        timer = GetClosestBlockOfType&lt;IMyTimerBlock&gt;(loopTimerName, landingTimerName); 
        var shipController = GetClosestBlockOfType&lt;IMyShipController&gt;(); 
        if (timer == null) 
        { 
            Echo($"CRITICAL: No timer named {loopTimerName} found\n Drop Sequence Aborted"); 
            return; 
        } 
        else if (!GetBlocks()) 
        { 
            return; 
        } 
        else if (shipController.GetNaturalGravity().LengthSquared() == 0) 
        { 
            Echo("CRITICAL: No natural gravity field detected\n Drop Sequence Aborted"); 
            return; 
        } 
        else 
        { 
            timer?.ApplyAction("TriggerNow"); 
            shouldDrop = true; 
            shouldTrigger = true; 
        } 
    } 
    else if (arg.ToLower() == "setup") 
    { 
        UpdateConfig(Me); 
        Echo("Configuration Updated"); 
        GetBlocks(); 
    } 
 
    if (shouldDrop &amp;&amp; !successfulDrop) 
    { 
        successfulDrop = ExecuteDrop(); 
    } 
 
    timeCurrentCycle += Runtime.TimeSinceLastRun.TotalSeconds; 
 
    if (timeCurrentCycle &gt;= timeMaxCycle) 
    { 
        runningSymbolVariant++; 
        Echo("WMI Drop Pod Systems... " + RunningSymbol()); 
 
        if (safeToActivate) 
        { 
            if (isSetup) //this should be false on first run 
            { 
                Echo("Drop Initiated"); 
                StabilizePod(); 
                StatusScreens(); 
 
                if (shouldBrake) 
                { 
                    if (downSpeed &gt; descentSpeed) 
                        BrakingOn(); 
                    else 
                    { 
                        if (attemptToLand) 
                            BrakingOff(); 
                        else 
                            ShutownSystems(); 
                    } 
                } 
                else 
                { 
                    BrakingOff(); 
                } 
 
                if (timeSpentStationary &gt; shutdownTime) 
                { 
                    ShutownSystems(); 
                } 
 
            } 
            else 
            { 
                isSetup = GetBlocks(); 
                Echo("Checking setup"); 
            } 
        } 
        else 
        { 
            Echo("Standing by for drop"); 
        } 
 
        timeCurrentCycle = 0; 
    } 
 
    if (successfulDrop &amp;&amp; !safeToActivate) 
    { 
        if (timeSinceDrop &gt; 0.2d) 
        { 
            safeToActivate = true; 
            GetBlocks(); //refetch blocks to make sure that we are only messing with blocks on this grid 
            ActivateBlocks(); 
        } 
        else 
            timeSinceDrop += Runtime.TimeSinceLastRun.TotalSeconds; 
    } 
 
    if (shouldTrigger) 
        timer?.ApplyAction("TriggerNow"); 
} 
 
bool ExecuteDrop() 
{ 
    var thisMerge = GetClosestBlockOfType&lt;IMyShipMergeBlock&gt;(mergeName); 
 
    //GridTerminalSystem.GetBlocksOfType&lt;&gt;() 
 
    if (thisMerge == null) 
    { 
        if (useMergeForDropPod) 
        { 
            Echo($"CRITICAL: No merges named '{mergeName}' were found\nDrop Sequence Aborted"); 
            shouldTrigger = false; 
            return false; 
        } 
        else 
        { 
            Echo($"Warning: No merges named '{mergeName}' were found"); 
            return true; 
        } 
    } 
 
    thisMerge?.ApplyAction("OnOff_Off"); //detach the drop pod merge if found 
    return true; 
} 
 
void ShutownSystems() 
{ 
    foreach (IMyThrust thisThrust in brakingThrusters) 
    { 
        thisThrust.SetValue("Override", 0f); 
        if (disableThrustOnLanding &amp;&amp; attemptToLand) 
            thisThrust.ApplyAction("OnOff_Off"); //changed from off 
        else 
            thisThrust.ApplyAction("OnOff_On"); //changed from off 
    } 
 
    foreach (IMyGyro thisGyro in gyros) 
    { 
        thisGyro.SetValue("Override", false); 
    } 
     
    foreach (IMyTimerBlock thisTimer in landingTimers) 
    { 
        thisTimer.ApplyAction("TriggerNow"); 
    } 
 
    referenceBlock.SetValue&lt;bool&gt;("DampenersOverride", true); 
 
    shouldTrigger = false; 
    Echo("Landing successful\nShutting down systems...\n\nGood Luck Pilot!"); 
     
    string rightScreenMessage = $" {shipName}\n--------------------------------------------------\nLanding successful\nShutting down systems...\n\nGood Luck Pilot!"; 
 
    //---Write to screens  
    statusScreens.Clear(); 
    GridTerminalSystem.GetBlocksOfType(statusScreens , block =&gt; block.CustomName.ToLower().Contains(statusScreenName.ToLower())); 
 
    foreach (IMyTextPanel thisScreen in statusScreens) 
    { 
        thisScreen.WritePublicText(rightScreenMessage); 
        thisScreen.ShowPublicTextOnScreen(); 
        //thisScreen.SetValue("FontSize", 1.5f); 
        thisScreen.SetValue&lt;long&gt;("Font", 1147350002); 
    } 
 
    //Me.ApplyAction("OnOff_Off"); 
} 
 
//Whip's Get Closest Block of Type Method variant 2 - 5/26/17 
//Added optional ignore name variable 
T GetClosestBlockOfType&lt;T&gt;(string name = "", string ignoreName = "") where T : class, IMyTerminalBlock 
{ 
    var allBlocks = new List&lt;T&gt;(); 
 
    if (name == "") 
    { 
        if (ignoreName == "") 
            GridTerminalSystem.GetBlocksOfType(allBlocks); 
        else 
            GridTerminalSystem.GetBlocksOfType(allBlocks, block =&gt; !block.CustomName.ToLower().Contains(ignoreName.ToLower())); 
    } 
    else 
    { 
        if (ignoreName == "") 
            GridTerminalSystem.GetBlocksOfType(allBlocks, block =&gt; block.CustomName.ToLower().Contains(name.ToLower())); 
        else 
            GridTerminalSystem.GetBlocksOfType(allBlocks, block =&gt; block.CustomName.ToLower().Contains(name.ToLower()) &amp;&amp; !block.CustomName.ToLower().Contains(ignoreName.ToLower())); 
    } 
 
    if (allBlocks.Count == 0) 
    { 
        return null; 
    } 
 
    var closestBlock = allBlocks[0]; 
    var shortestDistance = Vector3D.DistanceSquared(GetWorldPosition(Me), GetWorldPosition(closestBlock)); 
    allBlocks.Remove(closestBlock); //remove this block from the list 
 
    foreach (T thisBlock in allBlocks) 
    { 
        var thisDistance = Vector3D.DistanceSquared(GetWorldPosition(Me), GetWorldPosition(thisBlock)); 
 
        if (thisDistance &lt; shortestDistance) 
        { 
            closestBlock = thisBlock; 
            shortestDistance = thisDistance; 
        } 
        //otherwise move to next one 
    } 
 
    return closestBlock; 
} 
 
//Whip's Running Symbol Method v6 
int runningSymbolVariant = 0; 
string RunningSymbol() 
{ 
    string strRunningSymbol = ""; 
 
    if (runningSymbolVariant == 1) 
        strRunningSymbol = "|"; 
    else if (runningSymbolVariant == 2) 
        strRunningSymbol = "/"; 
    else if (runningSymbolVariant == 3) 
        strRunningSymbol = "--"; 
    else 
    { 
        strRunningSymbol = "\\"; 
        runningSymbolVariant = 0; 
    } 
 
    return strRunningSymbol; 
} 
 
double GetBrakingAltitudeThreshold() 
{ 
    double forceSum = 0; 
 
    foreach (IMyThrust thisThrust in brakingThrusters) 
    { 
        forceSum += thisThrust.MaxEffectiveThrust; 
    } 
 
    if (forceSum == 0) 
    { 
        return 1000d; //some arbitrary number that will stop NaN cases 
    } 
 
    //Echo($"Force: {forceSum.ToString()}"); 
    //Echo($"Speed: {downSpeed.ToString()}"); 
 
    double mass = referenceBlock.CalculateShipMass().PhysicalMass; 
 
    //Echo($"Mass: {mass.ToString()}"); 
    double deceleration = (forceSum / mass - gravityVecMagnitude); //Echo($"Decel: {deceleration.ToString()}"); 
 
    double safetyCushion = maxSpeed * timeMaxCycle * 1.5; //distance in meters that the drop pod will stop above the ground 
     
    //derived from: vf^2 = vi^2 + 2*a*d 
    double distanceToStop = shipVelocityVec.LengthSquared() / (2 * deceleration) + safetyCushion; //added for safety :) 
 
    return distanceToStop; 
} 
 
void ActivateBlocks() 
{ 
    foreach (IMyGyro thisGyro in gyros) 
    { 
        thisGyro.ApplyAction("OnOff_On"); 
    } 
 
    foreach (IMyThrust thisThruster in otherThrusters) 
    { 
        thisThruster.ApplyAction("OnOff_On"); 
    } 
} 
 
bool GetBlocks() 
{ 
    bool setupFailed = false; 
 
    gyros.Clear(); 
    allThrusters.Clear(); 
    brakingThrusters.Clear(); 
    otherThrusters.Clear(); 
    shipControllers.Clear(); 
    statusScreens.Clear(); 
    merges.Clear(); 
    landingTimers.Clear(); 
 
    GridTerminalSystem.GetBlocksOfType(shipControllers, block =&gt; block.CustomName.ToLower().Contains(referenceName.ToLower())); 
    GridTerminalSystem.GetBlocksOfType(gyros); 
    GridTerminalSystem.GetBlocksOfType(allThrusters); 
    GridTerminalSystem.GetBlocksOfType(landingTimers, block =&gt; block.CustomName.ToLower().Contains(landingTimerName.ToLower())); 
    timer = GetClosestBlockOfType&lt;IMyTimerBlock&gt;(loopTimerName, landingTimerName); 
 
    if (timer == null) 
    { 
        Echo($"CRITICAL: No timer named {loopTimerName} found\n Drop Sequence Aborted"); 
        setupFailed = true; 
    } 
 
    if (shipControllers.Count == 0) 
    { 
        setupFailed = true; 
        Echo($"CRITICAL: No ship controllers with the name '{referenceName}' were found"); 
    } 
    else 
    { 
        //---Assign our remote control 
        referenceBlock = shipControllers[0] as IMyShipController; 
        GetThrusterOrientation(referenceBlock); 
    } 
 
    if (brakingThrusters.Count == 0) 
    { 
        setupFailed = true; 
        Echo("CRITICAL: No downwards thrusters were found"); 
    } 
 
    if (gyros.Count == 0) 
    { 
        setupFailed = true; 
        Echo($"CRITICAL: No gyroscopes were found"); 
    } 
    else if (referenceBlock != null) 
    { 
        GetGyroOrientation(referenceBlock, gyros); 
    } 
     
    if (landingTimers.Count == 0) 
    { 
        Echo($"Optional: No landing timers found"); 
    } 
 
    if (setupFailed) 
    { 
        shouldTrigger = false; 
        Echo("Setup Failed!"); 
        return false; 
    } 
    else 
    { 
        Echo("Setup Successful!"); 
        return true; 
    } 
} 
 
void BrakingOn() 
{ 
    foreach (IMyThrust thisThrust in brakingThrusters) 
    { 
        thisThrust.ApplyAction("OnOff_On"); 
        thisThrust.SetValue("Override", 100f); 
    } 
} 
 
void BrakingOff() 
{ 
    foreach (IMyThrust thisThrust in brakingThrusters) 
    { 
        thisThrust.ApplyAction("OnOff_Off"); 
        thisThrust.SetValue("Override", 0f); 
    } 
} 
 
void StabilizePod() 
{ 
    //---Get speed 
    currentSpeed = referenceBlock.GetShipSpeed(); 
     
    //---Dir'n vectors of the reference block  
    var referenceMatrix = GetWorldMatrix(referenceBlock); 
    var referenceForward = referenceMatrix.Forward; 
    var referenceLeft = referenceMatrix.Left; 
    var referenceUp = referenceMatrix.Up; 
    var referenceOrigin = referenceMatrix.Translation; 
 
    //---Get gravity vector     
    gravityVec = referenceBlock.GetNaturalGravity(); 
    gravityVecMagnitude = gravityVec.Length(); 
    if (gravityVec.LengthSquared() == 0) 
    { 
        foreach (IMyGyro thisGyro in gyros) 
        { 
            thisGyro.SetValue("Override", false); 
        } 
        shouldStabilize = false; 
 
        angleRoll = 0; angleRoll = 0; 
        downSpeed = 0; 
        return; 
    } 
 
    shipVelocityVec = referenceBlock.GetShipVelocities().LinearVelocity; 
    downSpeed = VectorProjection(shipVelocityVec, gravityVec).Length() * Math.Sign(shipVelocityVec.Dot(gravityVec)); 
     
    //---Determine if we should manually override brake controls 
    altitude = 0; 
    referenceBlock.TryGetPlanetElevation(MyPlanetElevation.Surface, out altitude); 
    brakeAltitudeThreshold = GetBrakingAltitudeThreshold(); 
    stabilizeAltitudeThreshold = brakeAltitudeThreshold + 5 * currentSpeed; //this gives us a good safety cushion for stabilization procedures 
 
    //Echo($"Braking distance: {Math.Round(brakeAltitudeThreshold).ToString()}"); 
 
    if (altitude &lt; 100 &amp;&amp; currentSpeed &lt; 1) 
    { 
        timeSpentStationary += timeCurrentCycle; 
    } 
    else 
    { 
        timeSpentStationary = 0; 
 
        if (altitude &lt;= stabilizeAltitudeThreshold) 
        { 
            shouldStabilize = true; 
        } 
        else 
        { 
            shouldStabilize = false; 
        } 
 
        if (altitude &lt;= brakeAltitudeThreshold * 1.1) 
        { 
            shouldBrake = true; 
 
            if (referenceBlock.DampenersOverride) //kills dampeners to stop their interference with landing procedures 
            { 
                referenceBlock.SetValue&lt;bool&gt;("DampenersOverride", false); 
            } 
        } 
        else 
        { 
            shouldBrake = false; 
        } 
    } 
 
    Vector3D alignmentVec = new Vector3D(0, 0, 0); 
    //--Check if drift compensation is on 
    if (useDriftCompensation &amp;&amp; currentSpeed &gt; descentSpeed) 
    { 
        alignmentVec = shipVelocityVec; 
    } 
    else 
    { 
        alignmentVec = gravityVec; 
    } 
 
    //---Get Roll and Pitch Angles  
    anglePitch = Math.Acos(MathHelper.Clamp(alignmentVec.Dot(referenceForward) / alignmentVec.Length(), -1, 1)) - Math.PI / 2; 
 
    Vector3D planetRelativeLeftVec = referenceForward.Cross(alignmentVec);                                                                                                                   //w.H.i.p.L.A.s.h.1.4.1 
    angleRoll = Math.Acos(MathHelper.Clamp(referenceLeft.Dot(planetRelativeLeftVec) / planetRelativeLeftVec.Length(), -1, 1)); 
    angleRoll *= Math.Sign(VectorProjection(referenceLeft, alignmentVec).Dot(alignmentVec)); //ccw is positive  
 
    anglePitch *= -1; angleRoll *= -1; 
 
    roll_deg = Math.Round(angleRoll / Math.PI * 180); 
    pitch_deg = Math.Round(anglePitch / Math.PI * 180); 
 
    //---Angle controller     
    double rollSpeed = Math.Round(angleRoll, 2); 
    double pitchSpeed = Math.Round(anglePitch, 2); 
    //double rollSpeed = Math.Round(angleRoll / timeMaxCycle / 10, 2); 
    //double pitchSpeed = Math.Round(anglePitch / timeMaxCycle / 10, 2); 
 
    //---Enforce rotation speed limit 
    if (Math.Abs(rollSpeed) + Math.Abs(pitchSpeed) &gt; 2 * Math.PI) 
    { 
        double scale = 2 * Math.PI / (Math.Abs(rollSpeed) + Math.Abs(pitchSpeed)); 
        rollSpeed *= scale; 
        pitchSpeed *= scale; 
    } 
 
    //rollSpeed = rollSpeed / gyros.Count; //scales the overrides down so that we do not over rotate 
    //pitchSpeed = pitchSpeed / gyros.Count; 
 
    //---Set appropriate gyro override   
    if (shouldStabilize) 
    { 
        //GetGyroOrientation(referenceBlock, gyros); 
        ApplyGyroOverride(pitchSpeed, 0, -rollSpeed, gyros); 
    } 
    else 
    { 
        foreach (IMyGyro thisGyro in gyros) 
        { 
            thisGyro.SetValue("Override", false); 
        } 
    } 
} 
 
void StatusScreens() 
{ 
    //---Left Screen Output 
    if (shouldStabilize) 
        stableStatus = "&lt;&lt; Active &gt;&gt;"; 
    else 
        stableStatus = "&gt;&gt; Disabled &lt;&lt;"; 
 
    if (shouldBrake) 
        brakeStatus = "&lt;&lt; Active &gt;&gt;"; 
    else 
        brakeStatus = "&gt;&gt; Disabled &lt;&lt;"; 
 
    //---Right Screen Output  
    string rightScreenMessage = $" {shipName}\n--------------------------------------------------\n" 
+ $" Speed: {Math.Round(currentSpeed, 2)} m/s" 
+ $"\n\n Altidude: {Math.Round(altitude)} m" 
+ $"\n\n Braking Altitude: {Math.Round(brakeAltitudeThreshold)} m" 
+ string.Format("\n\n Pitch: {0:000} | Roll: {1:000}", -pitch_deg, roll_deg) 
+ $"\n\n Stabilizer: {stableStatus} \n\n Brakes: {brakeStatus}"; 
 
    //---Write to screens  
    statusScreens.Clear(); 
    GridTerminalSystem.GetBlocksOfType(statusScreens , block =&gt; block.CustomName.ToLower().Contains(statusScreenName.ToLower())); 
 
    foreach (IMyTextPanel thisScreen in statusScreens) 
    { 
        thisScreen.WritePublicText(rightScreenMessage); 
        thisScreen.ShowPublicTextOnScreen(); 
        //thisScreen.SetValue("FontSize", 1.5f); 
        thisScreen.SetValue&lt;long&gt;("Font", 1147350002); 
    } 
} 
 
Vector3D VectorProjection(Vector3D a, Vector3D b) //proj a on b     
{ 
    Vector3D projection = a.Dot(b) / b.LengthSquared() * b; 
    return projection; 
} 
 
#region GYRO ORIENTATION 
//Whip's Gyro Orientation Method v4 - 6/2/17 
//Changed to use dictionary instead of array for redundancy incase blocks are destroyed 
//Also uses new world matrix workaround 
 
Dictionary&lt;IMyGyro, string&gt; gyroRelativeYaw = new Dictionary&lt;IMyGyro, string&gt;(); 
Dictionary&lt;IMyGyro, string&gt; gyroRelativePitch = new Dictionary&lt;IMyGyro, string&gt;(); 
Dictionary&lt;IMyGyro, string&gt; gyroRelativeRoll = new Dictionary&lt;IMyGyro, string&gt;(); 
Dictionary&lt;IMyGyro, int&gt; gyroYawSign = new Dictionary&lt;IMyGyro, int&gt;(); 
Dictionary&lt;IMyGyro, int&gt; gyroPitchSign = new Dictionary&lt;IMyGyro, int&gt;(); 
Dictionary&lt;IMyGyro, int&gt; gyroRollSign = new Dictionary&lt;IMyGyro, int&gt;(); 
 
void GetGyroOrientation(IMyTerminalBlock reference_block, List&lt;IMyGyro&gt; gyro_list) 
{ 
    gyroRelativeYaw = new Dictionary&lt;IMyGyro, string&gt;(gyro_list.Count); 
    gyroRelativePitch = new Dictionary&lt;IMyGyro, string&gt;(gyro_list.Count); 
    gyroRelativeRoll = new Dictionary&lt;IMyGyro, string&gt;(gyro_list.Count); 
 
    gyroYawSign = new Dictionary&lt;IMyGyro, int&gt;(gyro_list.Count); 
    gyroPitchSign = new Dictionary&lt;IMyGyro, int&gt;(gyro_list.Count); 
    gyroRollSign = new Dictionary&lt;IMyGyro, int&gt;(gyro_list.Count); 
 
    var referenceMatrix = GetWorldMatrix(reference_block); 
    var reference_up = referenceMatrix.Up; //assuming rot right 
    var reference_right = referenceMatrix.Right; //assuming rot up 
    var reference_forward = referenceMatrix.Forward; //assuming rot up 
 
    foreach (var thisGyro in gyro_list) 
    { 
        var gyroMatrix = GetWorldMatrix(thisGyro); 
        var gyro_forward = gyroMatrix.Forward; 
        var gyro_backward = gyroMatrix.Backward; 
        var gyro_up = gyroMatrix.Up; 
        var gyro_down = gyroMatrix.Down; 
        var gyro_left = gyroMatrix.Left; 
        var gyro_right = gyroMatrix.Right; 
 
        /// Pitch Fields ///    
        if (reference_right == gyro_forward) 
        { 
            gyroRelativePitch.Add(thisGyro, "Roll"); 
            gyroPitchSign.Add(thisGyro, 1); 
        } 
        else if (reference_right == gyro_backward) 
        { 
            gyroRelativePitch.Add(thisGyro, "Roll"); 
            gyroPitchSign.Add(thisGyro, -1); 
        } 
        else if (reference_right == gyro_right) 
        { 
            gyroRelativePitch.Add(thisGyro, "Pitch"); 
            gyroPitchSign.Add(thisGyro, 1); 
        } 
        else if (reference_right == gyro_left) 
        { 
            gyroRelativePitch.Add(thisGyro, "Pitch"); 
            gyroPitchSign.Add(thisGyro, -1); 
        } 
        else if (reference_right == gyro_up) 
        { 
            gyroRelativePitch.Add(thisGyro, "Yaw"); 
            gyroPitchSign.Add(thisGyro, -1); 
        } 
        else if (reference_right == gyro_down) 
        { 
            gyroRelativePitch.Add(thisGyro, "Yaw"); 
            gyroPitchSign.Add(thisGyro, 1); 
        } 
 
        /// Yaw Fields /// 
        if (reference_up == gyro_forward) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Roll"); 
            gyroYawSign.Add(thisGyro, -1); 
        } 
        else if (reference_up == gyro_backward) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Roll"); 
            gyroYawSign.Add(thisGyro, 1); 
        } 
        else if (reference_up == gyro_right) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Pitch"); 
            gyroYawSign.Add(thisGyro, -1); 
        } 
        else if (reference_up == gyro_left) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Pitch"); 
            gyroYawSign.Add(thisGyro, 1); 
        } 
        else if (reference_up == gyro_up) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Yaw"); 
            gyroYawSign.Add(thisGyro, 1); 
        } 
        else if (reference_up == gyro_down) 
        { 
            gyroRelativeYaw.Add(thisGyro, "Yaw"); 
            gyroYawSign.Add(thisGyro, -1); 
        } 
 
        /// Roll Fields /// 
        if (reference_forward == gyro_forward) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Roll"); 
            gyroRollSign.Add(thisGyro, 1); 
        } 
        else if (reference_forward == gyro_backward) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Roll"); 
            gyroRollSign.Add(thisGyro, -1); 
        } 
        else if (reference_forward == gyro_right) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Pitch"); 
            gyroRollSign.Add(thisGyro, 1); 
        } 
        else if (reference_forward == gyro_left) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Pitch"); 
            gyroRollSign.Add(thisGyro, -1); 
        } 
        else if (reference_forward == gyro_up) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Yaw"); 
            gyroRollSign.Add(thisGyro, -1); 
        } 
        else if (reference_forward == gyro_down) 
        { 
            gyroRelativeRoll.Add(thisGyro, "Yaw"); 
            gyroRollSign.Add(thisGyro, 1); 
        } 
    } 
} 
 
void ApplyGyroOverride(double pitch_speed, double yaw_speed, double roll_speed, List&lt;IMyGyro&gt; gyro_list) 
{ 
    foreach (var thisGyro in gyro_list) 
    { 
        string relativeYaw, relativePitch, relativeRoll; 
        int yawSign, pitchSign, rollSign; 
 
        bool test1 = gyroRelativeYaw.TryGetValue(thisGyro, out relativeYaw); 
        bool test2 = gyroRelativePitch.TryGetValue(thisGyro, out relativePitch); 
        bool test3 = gyroRelativeRoll.TryGetValue(thisGyro, out relativeRoll); 
        bool test4 = gyroYawSign.TryGetValue(thisGyro, out yawSign); 
        bool test5 = gyroPitchSign.TryGetValue(thisGyro, out pitchSign); 
        bool test6 = gyroRollSign.TryGetValue(thisGyro, out rollSign); 
 
        if (test1 &amp;&amp;test2 &amp;&amp; test3 &amp;&amp; test4 &amp;&amp; test5 &amp;&amp; test6) 
        { 
            thisGyro.SetValue&lt;float&gt;(relativeYaw, (float)yaw_speed * yawSign); 
            thisGyro.SetValue&lt;float&gt;(relativePitch, (float)pitch_speed * pitchSign); 
            thisGyro.SetValue&lt;float&gt;(relativeRoll, (float)roll_speed * rollSign); 
            thisGyro.SetValue("Override", true); 
        } 
    } 
} 
